<?xml version="1.0" encoding="UTF-8"?>
<web-app
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"

	id="jsonix-tests-selene" version="2.5">

	<display-name>Jsonix Tests [Selene]</display-name>

	<filter>
		<filter-name>net.disy.legato.tools.servlet.ResourceMappingFilter</filter-name>
		<filter-class>net.disy.legato.tools.servlet.ResourceMappingFilter</filter-class>
		<init-param>
			<param-name>filterPath</param-name>
			<param-value>js/</param-value>
		</init-param>
	</filter>

	<filter-mapping>
		<filter-name>net.disy.legato.tools.servlet.ResourceMappingFilter</filter-name>
		<url-pattern>/js/*</url-pattern>
	</filter-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>